{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build slotted page",
            "command": "/opt/homebrew/bin/g++-11",
            "args": [
                "-g",
                "slot_header.cpp",
                "page.cpp",
                "main_slotted_page.cpp",
                "-o",
                "page"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "build B-tree",
            "command": "/opt/homebrew/bin/g++-11",
            "args": [
                "-g",
                "slot_header.cpp",
                "page.cpp",
                "main_btree.cpp",
                "btree.cpp",
                "-o",
                "btree"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}